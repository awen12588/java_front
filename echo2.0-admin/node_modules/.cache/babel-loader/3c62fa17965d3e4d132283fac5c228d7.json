       <el-table-column label=\"跳转地址\" align=\"center\" prop=\"linkUrl\" />\r\n        <el-table-column label=\"排序\" align=\"center\" prop=\"sort\" />\r\n\r\n        <el-table-column label=\"状态\" align=\"center\" prop=\"isOpen\">\r\n          <template slot-scope=\"scope\">\r\n            <el-tag\r\n              type=\"success\"\r\n              size=\"mini\"\r\n              plain\r\n              v-if=\"scope.row.isOpen == true\"\r\n              >开启</el-tag\r\n            >\r\n            <el-tag\r\n              type=\"info\"\r\n              size=\"mini\"\r\n              plain\r\n              v-if=\"scope.row.isOpen == false\"\r\n              >关闭</el-tag\r\n            >\r\n          </template>\r\n        </el-table-column>\r\n        <el-table-column\r\n          label=\"操作\"\r\n          fixed=\"right\"\r\n          width=\"150\"\r\n          align=\"center\"\r\n          class-name=\"small-padding fixed-width\"\r\n        >\r\n          <template slot-scope=\"scope\">\r\n            <el-button\r\n              size=\"small\"\r\n              plain\r\n              type=\"primary\"\r\n              @click=\"handleChange(scope.row)\"\r\n              v-hasPermi=\"['bussiness:user:edit']\"\r\n              >修改</el-button\r\n            >\r\n\r\n            <el-button\r\n              size=\"small\"\r\n              plain\r\n              type=\"danger\"\r\n              @click=\"handleDelete(scope.row)\"\r\n              v-hasPermi=\"['bussiness:user:remove']\"\r\n              >删除</el-button\r\n            >\r\n          </template>\r\n        </el-table-column>\r\n      </el-table>\r\n    </TableContentBox>\r\n\r\n    <!-- 添加或修改底部栏 -->\r\n    <el-dialog :title=\"title\" :visible.sync=\"open\" width=\"500px\" append-to-body @close=\"close\">\r\n      <el-form ref=\"form\" :model=\"form\" :rules=\"rules\" label-width=\"80px\">\r\n        <el-form-item label=\"名称\" prop=\"name\">\r\n          <el-input v-model=\"form.name\"\r\n        /></el-form-item>\r\n\